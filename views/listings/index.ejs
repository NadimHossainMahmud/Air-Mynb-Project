<% layout("./layouts/boilerplate.ejs")%>
<body>
  <h2>All listings</h2>
  <form action="/listings/new">
    <button>Add new listings</button>
  </form>
  <ul>
    <% for (let listing of allListings){ %>
    <li><a href="/listings/<%= listing._id %>"><%= listing.title%></a></li>
    <% } %>
  </ul>
</body>
